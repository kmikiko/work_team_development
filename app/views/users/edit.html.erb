<h1>ユーザー情報編集</h1>

<%= form_with(model: @user, local: true) do |f| %>  
  <%= f.label :ユーザー名 %>
  <%= f.text_field :user_name %>

  <%= f.label :メールアドレス %>
  <%= f.email_field :email %>

  <%= f.label :プロフィール %>
  <%= f.text_field :profile %>

  <%= f.label :パスワード %>
  <%= f.password_field :password %>

  <%= f.label :確認用パスワード %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "変更" %>
<% end %>